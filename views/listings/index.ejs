<% layout("/layouts/boilerplate") %>
<body>
  <h1>All Listings</h1>

  <ul>
    <% allListings.forEach((listing) => { %>
      <li>
        <a href="/listings/<%= listing._id %>"><%= listing.title %></a>
        - <%= listing.price %> Rs
        - <%= listing.location %>, <%= listing.country %>
      </li>
    <% }) %>
  </ul>

  <a href="/listings/new">Add New Listing</a>
</body>